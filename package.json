{
  "name": "foxgis-server-lite",
  "version": "1.0.0",
  "private": true,
  "bin": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "lint": "eslint *.js --fix",
    "test": "npm run lint",
    "pkg": "pkg . --out-path=dist/"
  },
  "engines": {
    "node": ">=6"
  },
  "pkg": {
    "assets": [
      "node_modules/@mapbox/mbtiles/lib/schema.sql",
      "node_modules/@mapbox/glyph-pbf-composite/proto/glyphs.proto",
      "node_modules/mapnik/lib/binding/mapnik_settings.js"
    ],
    "targets": [
      "node6-macos-x64",
      "node6-linux-x64",
      "node6-win-x64"
    ]
  },
  "dependencies": {
    "@mapbox/glyph-pbf-composite": "^0.0.3",
    "@mapbox/mbtiles": "^0.9.0",
    "@mapbox/spritezero": "^5.1.0",
    "async": "^2.6.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.1",
    "debug": "^3.0.0",
    "express": "^4.16.2",
    "helmet": "^3.1.0",
    "method-override": "^2.3.7",
    "morgan": "^1.8.2",
    "rd": "^1.1.2"
  },
  "devDependencies": {
    "eslint": "^4.14.0",
    "pkg": "^4.3.0-beta.5"
  }
}
